{
  "name": "wind-layer",
  "version": "0.0.5",
  "description": "a openlayers extension to windjs",
  "author": "FDD <smileFDD@gmail.com>",
  "homepage": "https://sakitam-fdd.github.io/wind-layer",
  "private": false,
  "main": "dist/windLayer.common.js",
  "module": "dist/windLayer.esm.js",
  "unpkg": "dist/windLayer.js",
  "namespace": "windLayer",
  "repository": {
    "type": "git",
    "url": "https://github.com/sakitam-fdd/wind-layer.git"
  },
  "bugs": {
    "url": "https://github.com/sakitam-fdd/wind-layer/issues"
  },
  "keywords": [
    "javascript",
    "webgis",
    "canvas",
    "windjs",
    "openlayers",
    "node",
    "grib"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=dev input=index.js file=windLayer rollup -wm -c build/rollup-dev-config.js",
    "dev:ol": "cross-env NODE_ENV=dev input=openlayers/index.js file=OlWind rollup -wm -c build/rollup-dev-config.js",
    "dev:bmap": "cross-env NODE_ENV=dev input=bmap/index.js file=BMapWind rollup -wm -c build/rollup-dev-config.js",
    "dev:amap": "cross-env NODE_ENV=dev input=amap/index.js file=AMapWind rollup -wm -c build/rollup-dev-config.js",
    "build:ol": "cross-env NODE_ENV=pro input=openlayers/index.js file=OlWind rollup -m -c build/rollup-dev-config.js",
    "build:amap": "cross-env NODE_ENV=pro input=amap/index.js file=AMapWind rollup -m -c build/rollup-dev-config.js",
    "build:bmap": "cross-env NODE_ENV=pro input=bmap/index.js file=BMapWind rollup -m -c build/rollup-dev-config.js",
    "build": "npm run build:ol && npm run build:bmap && npm run build:amap && cross-env NODE_ENV=pro input=index.js file=windLayer node build/rollup-prod-config.js",
    "start": "node serve/bin/www",
    "dev:server": "nodemon -w server server/bin/www",
    "prd:server": "pm2 start server/bin/www"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.20.1"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-proto-to-assign": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "child_process": "^1.0.2",
    "cross-env": "^5.2.0",
    "debug": "^2.6.3",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs": "^0.0.1-security",
    "koa": "^2.4.1",
    "koa-body": "^2.5.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.3.0",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "nodemon": "^1.12.5",
    "pm2": "^2.9.2",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^3.3.13"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "license": "MIT"
}
